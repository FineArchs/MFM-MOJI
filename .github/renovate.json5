{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  dependencyDashboard: true,
  prConcurrentLimit: 0,
  configMigration: true,
  separateMultipleMajor: true,
  extends: [
    'group:allNonMajor',
  ],
  packageRules: [
    {
      major: {
        enabled: true,
      },
      groupName: 'stylelint',
      matchPackagePatterns: '.*stylelint.*',
    },
  ],
}
