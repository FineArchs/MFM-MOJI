{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  dependencyDashboard: true,
  prConcurrentLimit: 0,
  configMigration: true,
  extends: [
    'group:allNonMajor',
  ],
  packageRules: [
    {
      major: {
        enabled: true,
      },
      separateMultipleMajor: true,
      groupName: 'stylelint',
      matchDepNames: 'stylelint',
    },
  ],
}
